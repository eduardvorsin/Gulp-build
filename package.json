	"scripts": {
		"dev": "gulp",
		"build": "npm run stylelint-fix && npm run eslint-fix && gulp build --build",
		"stylelint-fix": "stylelint \"#source/scss/**/*.scss\" --fix",
		"eslint-fix": "eslint \"#source/js/**/*.js\" --fix",
		"lighthouse-desktop-test": "lighthouse http://localhost:3000/ --config-path=./lighthouse-config.js --output html --output-path ./lighthouseReports/lighthouse-desktop-reports.html --view",
		"lighthouse-mobile-test": "lighthouse http://localhost:3000/ --config-path=./lighthouse-config.js --output html --output-path ./lighthouseReports/lighthouse-mobile-reports.html --view --mobile"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
